# Supabase Configuration
# Get these from your Supabase project dashboard
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# n8n Webhook URLs
# Get these from your n8n instance after importing workflows
NEXT_PUBLIC_N8N_SOCIAL_SHARE_WEBHOOK=https://your-n8n-instance.com/webhook/social-share
N8N_PROCESS_IMAGE_WEBHOOK=https://your-n8n-instance.com/webhook/process-tryon-image
N8N_USER_SIGNUP_WEBHOOK=https://your-n8n-instance.com/webhook/user-signup

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Analytics (Optional)
NEXT_PUBLIC_POSTHOG_KEY=your-posthog-key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Sentry (Optional)
SENTRY_DSN=your-sentry-dsn

# AI Services (for Edge Functions)
REPLICATE_API_TOKEN=your-replicate-api-token
OPENAI_API_KEY=your-openai-api-key
STABILITY_API_KEY=your-stability-api-key
