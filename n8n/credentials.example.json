{
  "credentials": [
    {
      "id": "1",
      "name": "Redis account",
      "type": "redis",
      "data": {
        "host": "your-redis-host.com",
        "port": 6379,
        "password": "your-redis-password",
        "database": 0
      }
    },
    {
      "id": "2",
      "name": "Twitter account",
      "type": "twitterOAuth2Api",
      "data": {
        "clientId": "your-twitter-client-id",
        "clientSecret": "your-twitter-client-secret",
        "accessToken": "your-twitter-access-token",
        "refreshToken": "your-twitter-refresh-token"
      }
    },
    {
      "id": "3",
      "name": "Instagram account",
      "type": "instagramBusinessApi",
      "data": {
        "accessToken": "your-instagram-access-token",
        "accountId": "your-instagram-business-account-id"
      }
    },
    {
      "id": "4",
      "name": "Pinterest account",
      "type": "pinterestOAuth2Api",
      "data": {
        "clientId": "your-pinterest-client-id",
        "clientSecret": "your-pinterest-client-secret",
        "accessToken": "your-pinterest-access-token"
      }
    },
    {
      "id": "5",
      "name": "Supabase PostgreSQL",
      "type": "postgres",
      "data": {
        "host": "db.your-project-ref.supabase.co",
        "port": 5432,
        "database": "postgres",
        "user": "postgres",
        "password": "your-supabase-db-password",
        "ssl": {
          "rejectUnauthorized": false
        }
      }
    },
    {
      "id": "6",
      "name": "PostHog API",
      "type": "httpHeaderAuth",
      "data": {
        "name": "Authorization",
        "value": "Bearer your-posthog-api-key"
      }
    },
    {
      "id": "7",
      "name": "SMTP account",
      "type": "smtp",
      "data": {
        "host": "smtp.your-email-provider.com",
        "port": 587,
        "secure": false,
        "user": "your-smtp-username",
        "password": "your-smtp-password",
        "fromEmail": "noreply@beautytry-on.app"
      }
    },
    {
      "id": "8",
      "name": "Supabase Storage Auth",
      "type": "httpHeaderAuth",
      "data": {
        "name": "Authorization",
        "value": "Bearer your-supabase-service-role-key"
      }
    },
    {
      "id": "9",
      "name": "Supabase Functions Auth",
      "type": "httpHeaderAuth",
      "data": {
        "name": "Authorization",
        "value": "Bearer your-supabase-anon-key"
      }
    }
  ],
  "notes": {
    "setup_instructions": [
      "1. Copy this file to credentials.json",
      "2. Replace all placeholder values with your actual credentials",
      "3. DO NOT commit credentials.json to version control",
      "4. Import credentials.json into your n8n instance",
      "5. Ensure all workflow nodes reference the correct credential IDs"
    ],
    "credential_sources": {
      "Redis": "Get from your Redis provider (e.g., Upstash, Redis Cloud)",
      "Twitter/X": "https://developer.twitter.com/en/portal/dashboard",
      "Instagram": "https://developers.facebook.com/apps/ (requires Facebook Business account)",
      "Pinterest": "https://developers.pinterest.com/apps/",
      "Supabase": "Your Supabase project dashboard > Settings > Database / API",
      "PostHog": "https://app.posthog.com/project/settings",
      "SMTP": "Your email service provider (SendGrid, Mailgun, AWS SES, etc.)"
    },
    "environment_variables": {
      "POSTHOG_API_KEY": "Set in n8n environment or use credential",
      "SUPABASE_URL": "https://your-project-ref.supabase.co",
      "SUPABASE_ANON_KEY": "Your Supabase anonymous key",
      "SUPABASE_SERVICE_ROLE_KEY": "Your Supabase service role key (keep secret!)"
    }
  }
}
